  //1) Write a JavaScript program to concatenate three strings ”Welcome”, ”to”,
      //and ”JavaScript” with spaces between them and log the result.

      let a = "welcome"
      let b = "to"
      let c = "Javascript"

      let d = a + b +c ; 
      console.log(d);
      
      let d = `${a} ${b} ${c}`;
      console.log(d);
      console.log(typeof(d));
      

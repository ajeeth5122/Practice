      //Named Function

      function sayHi() {
         console.log("Hi Everyone!")
      }
      
      sayHi();

      // Anonymous Function

      let addNumbers = function (a,b) {
         console.log(a*b)
      }

      addNumbers(20,10);

      //Arrow Function

      let doubleNum = 20;

      let doubled = (twoTimes)=>{
         return twoTimes**2
      }
      let Output1 = doubled(doubleNum);

      console.log(Output1);


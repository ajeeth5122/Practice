      //5)

      let name2="sachin"
      let name3="tendulkar"
       

      function greetwithAge(a,b=20) {
         console.log(`${a} is ${b}years old`)  
      }
      

      greetwithAge(name2,25);
      greetwithAge(name3)

      

      //8 Write a named function print Message that logs ”JavaScript is fun!”
